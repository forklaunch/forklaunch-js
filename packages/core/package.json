    "@types/uuid": "^10.0.0",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "testcontainers": "^10.10.1",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.15.0"
