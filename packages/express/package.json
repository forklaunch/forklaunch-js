{
  "name": "@forklaunch/express",
  "version": "0.1.0",
  "description": "Forklaunch framework for express.",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "clean": "rm -rf dist pnpm.lock.yaml node_modules",
    "docs": "typedoc --out docs *",
    "lint": "eslint . -c eslint.config.mjs",
    "lint:fix": "eslint . -c eslint.config.mjs --fix",
    "format": "prettier --ignore-path=.prettierignore --config .prettierrc '**/*.ts' --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forklaunch/forklaunch-js.git"
  },
  "author": "Rohin Bhargava",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/forklaunch/forklaunch-js/issues"
  },
  "homepage": "https://github.com/forklaunch/forklaunch-js#readme",
  "dependencies": {
    "@forklaunch/core": "^0.2.2",
    "@forklaunch/validator": "^0.3.1",
    "@types/express": "^4.17.21",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "qs": "^6.12.3",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.12",
    "@types/qs": "^6.9.15",
    "@types/swagger-ui-express": "^4.1.6",
    "kill-port-process": "^3.2.1",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.0"
  }
}