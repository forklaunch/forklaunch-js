{
  "name": "@{{ app_name }}/core",
  "version": "0.1.0",
  "description": "The common core lib for all monorepo services for {{ app_name }}",
  "files": [
    "lib"
  ],
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "{{ test-framework }} --passWithNoTests",
    "build": "tsc",
    "clean": "rm -rf lib pnpm.lock.yaml node_modules",
    "docs": "typedoc --out docs *",
    "lint": "eslint . -c eslint.config.mjs",
    "lint:fix": "eslint . -c eslint.config.mjs --fix",
    "format": "prettier --ignore-path=.prettierignore --config .prettierrc '**/*.ts' --write"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@forklaunch/common": "^0.1.11",
    "@forklaunch/core": "^0.2.27",
    "@forklaunch/express": "^0.1.22",
    "@forklaunch/hyper-express": "^0.1.23",
    "@forklaunch/validator": "^0.3.9",
    "@sinclair/typebox": "^0.33.22",
    "zod": "^3.23.8"
  }
}