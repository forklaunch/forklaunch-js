{
  "name": "@forklaunch/blueprint-core",
  "version": "1.0.0",
  "description": "The common core lib for all monorepo services",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib pnpm.lock.yaml node_modules",
    "docs": "typedoc --out docs *",
    "format": "prettier --ignore-path=.prettierignore --config .prettierrc '**/*.{ts,tsx,json}' --write",
    "lint": "eslint . -c eslint.config.mjs",
    "lint:fix": "eslint . -c eslint.config.mjs --fix",
    "prepack": "pnpm run build",
    "test": "vitest --passWithNoTests"
  },
  "dependencies": {
    "@forklaunch/common": "^0.6.26",
    "@forklaunch/core": "^0.17.2",
    "@forklaunch/express": "^0.10.2",
    "@forklaunch/hyper-express": "^0.11.2",
    "@forklaunch/validator": "^0.10.26",
    "@mikro-orm/core": "^6.6.4",
    "@mikro-orm/mongodb": "^6.6.4",
    "@sinclair/typebox": "^0.34.47",
    "uuid": "^13.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/qs": "^6.14.0",
    "@types/uuid": "^11.0.0"
  }
