{
  "name": "@forklaunch/blueprint-core",
  "version": "1.0.0",
  "description": "The common core lib for all monorepo services",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib pnpm.lock.yaml node_modules",
    "docs": "typedoc --out docs *",
    "format": "prettier --ignore-path=.prettierignore --config .prettierrc '**/*.{ts,tsx,json}' --write",
    "lint": "eslint . -c eslint.config.mjs",
    "lint:fix": "eslint . -c eslint.config.mjs --fix",
    "prepack": "pnpm run build",
    "test": "vitest --passWithNoTests"
  },
  "dependencies": {
    "@forklaunch/common": "^0.6.18",
    "@forklaunch/core": "^0.15.7",
    "@forklaunch/express": "^0.9.25",
    "@forklaunch/hyper-express": "^0.9.25",
    "@forklaunch/validator": "^0.10.18",
    "@mikro-orm/core": "^6.5.7",
    "@mikro-orm/mongodb": "^6.5.7",
    "@sinclair/typebox": "^0.34.41",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/qs": "^6.14.0",
    "@types/uuid": "^11.0.0"
  }
