{
  "name": "@forklaunch/hyper-express",
  "version": "0.9.17",
  "description": "Forklaunch framework for hyper-express.",
  "homepage": "https://github.com/forklaunch/forklaunch-js#readme",
  "bugs": {
    "url": "https://github.com/forklaunch/forklaunch-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forklaunch/forklaunch-js.git"
  },
  "license": "MIT",
  "author": "Rohin Bhargava",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "types": "lib/index.d.ts",
  "files": [
    "lib/**"
  ],
  "scripts": {
    "build": "tsgo --noEmit && tsup index.ts --format cjs,esm --no-splitting --dts --tsconfig tsconfig.json --out-dir lib --clean",
    "clean": "rm -rf lib pnpm.lock.yaml node_modules",
    "docs": "typedoc --out docs *",
    "format": "prettier --ignore-path=.prettierignore --config .prettierrc '**/*.{ts,json}' --write",
    "lint": "eslint . -c eslint.config.mjs",
    "lint:fix": "eslint . -c eslint.config.mjs --fix",
    "prepack": "pnpm run build",
    "publish:package": "./publish-package.bash",
    "test": "vitest --passWithNoTests"
  },
  "dependencies": {
    "@forklaunch/common": "workspace:*",
    "@forklaunch/core": "workspace:*",
    "@forklaunch/hyper-express-fork": "^6.17.35",
    "@forklaunch/validator": "workspace:*",
    "@scalar/express-api-reference": "^0.8.18",
    "cors": "^2.8.5",
    "live-directory": "^3.0.3",
    "openapi3-ts": "^4.5.0",
    "qs": "^6.14.0",
    "swagger-ui-dist": "^5.29.0",
    "swagger-ui-express": "^5.0.1",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.52.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/busboy": "^1.5.4",
    "@types/cors": "^2.8.19",
    "@types/jest": "^30.0.0",
    "@types/qs": "^6.14.0",
    "@types/swagger-ui-dist": "^3.30.6",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript/native-preview": "7.0.0-dev.20250917.1",
    "jest": "^30.1.3",
    "kill-port-process": "^3.2.1",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typedoc": "^0.28.13",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0"
  }
}
