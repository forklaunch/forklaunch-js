{
  "name": "@forklaunch/express",
  "version": "0.10.1",
  "description": "Forklaunch framework for express.",
  "homepage": "https://github.com/forklaunch/forklaunch-js#readme",
  "bugs": {
    "url": "https://github.com/forklaunch/forklaunch-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forklaunch/forklaunch-js.git"
  },
  "license": "MIT",
  "author": "Rohin Bhargava",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "types": "lib/index.d.ts",
  "files": [
    "lib/**"
  ],
  "scripts": {
    "build": "tsgo --noEmit && tsup index.ts --format cjs,esm --no-splitting --tsconfig tsconfig.json --outDir lib --dts --clean",
    "clean": "rm -rf lib pnpm.lock.yaml node_modules",
    "docs": "typedoc --out docs *",
    "format": "prettier --ignore-path=.prettierignore --config .prettierrc '**/*.{ts,tsx,json}' --write",
    "lint": "eslint . -c eslint.config.mjs",
    "lint:fix": "eslint . -c eslint.config.mjs --fix",
    "prepack": "pnpm run build",
    "publish:package": "./publish-package.bash",
    "test": "vitest --passWithNoTests"
  },
  "dependencies": {
    "@forklaunch/common": "workspace:*",
    "@forklaunch/core": "workspace:*",
    "@forklaunch/validator": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@scalar/express-api-reference": "^0.8.34",
    "@types/multer": "^2.0.0",
    "body-parser": "^2.2.2",
    "busboy": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "fastmcp": "^3.26.9",
    "multer": "2.0.2",
    "qs": "^6.14.1",
    "range-parser": "^1.2.1",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/body-parser": "^1.19.6",
    "@types/bun": "^1.3.6",
    "@types/busboy": "^1.5.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/express-serve-static-core": "^5.1.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.9",
    "@types/qs": "^6.14.0",
    "@types/range-parser": "^1.2.7",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript/native-preview": "7.0.0-dev.20260118.1",
    "jest": "^30.2.0",
    "kill-port-process": "^3.2.1",
    "prettier": "^3.8.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0"
  }
}
